<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MusicPlayer.Views.MusicNavigationView"
			 xmlns:mi="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia">
	<Grid ColumnDefinitions="*,*,2*,*,*">
		<Button Grid.Column="0" HorizontalAlignment="Stretch">
			<mi:MaterialIcon Kind="ShuffleVariant"/>
		</Button>
		<Button Grid.Column="1" HorizontalAlignment="Stretch">
			<mi:MaterialIcon Kind="SkipPrevious"/>
		</Button>
		<Button Grid.Column="2" HorizontalAlignment="Stretch">
			<mi:MaterialIcon Kind="Play"/>
		</Button>
		<Button Grid.Column="3" HorizontalAlignment="Stretch">
			<mi:MaterialIcon Kind="SkipNext"/>
		</Button>
		<Button Grid.Column="4" HorizontalAlignment="Stretch">
			<mi:MaterialIcon Kind="RepeatVariant"/>
		</Button>
	</Grid>
</UserControl>
