<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:MusicPlayer.Views"
		xmlns:vm="clr-namespace:MusicPlayer.ViewModels"
		mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="50" Width="420" Height="90"
        x:Class="MusicPlayer.Views.ControlWidget"
		x:DataType="vm:MainViewModel"
        Title="ControlWidget"
		SystemDecorations="None"
		ShowInTaskbar="False"
		Topmost="True"
		>
	<Window.Background>
		<SolidColorBrush Opacity="0.5" Color="{DynamicResource SystemRegionColor}"/>
	</Window.Background>
	<UserControl Content="{Binding MusicNavigation}"/>

</Window>
